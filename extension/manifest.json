{
  "manifest_version": 3,
  "name": "Formless Autofill",
  "version": "0.1",
  "description": "Demo: AI-powered form autofill",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["fieldPicker.js", "fill.js"]
    }
  ]
}
